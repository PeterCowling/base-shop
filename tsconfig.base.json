{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2022",
    "module": "esnext",
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "moduleSuffixes": [
      ".server",
      ".client",
      ""
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "dist-types",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "node",
      "react",
      "react-dom"
    ],
    "paths": {
      "~test/*": [
        "test/*"
      ],
      "@acme/editorial": [
        "packages/editorial/src/index.ts",
        "packages/editorial/dist/index.d.ts"
      ],
      "@acme/editorial/*": [
        "packages/editorial/src/*",
        "packages/editorial/dist/*"
      ],
      "@acme/platform-machine": [
        "packages/platform-machine/src/index.ts",
        "packages/platform-machine/dist/index.d.ts"
      ],
      "@acme/platform-machine/*": [
        "packages/platform-machine/src/*",
        "packages/platform-machine/dist/*"
      ],
      "@acme/ui": [
        "packages/ui/src/index.ts",
        "packages/ui/dist/index.d.ts"
      ],
      "@acme/ui/*": [
        "packages/ui/src/*",
        "packages/ui/dist/*"
      ],
      "@cms": [
        "apps/cms/src",
        "apps/cms/dist/index.d.ts"
      ],
      "@cms/*": [
        "apps/cms/src/*",
        "apps/cms/dist/*"
      ],
      "@themes/*": [
        "packages/themes/*/src",
        "packages/themes/*/dist/index.d.ts"
      ],
      "@acme/lib": [
        "packages/lib/src/index.ts",
        "packages/lib/dist/index.d.ts"
      ],
      "@acme/lib/*": [
        "packages/lib/src/*",
        "packages/lib/dist/*"
      ],
      "@acme/i18n": [
        "packages/i18n/src/index.ts",
        "packages/i18n/dist/index.d.ts"
      ],
      "@acme/i18n/*": [
        "packages/i18n/src/*",
        "packages/i18n/dist/*"
      ],
      "@acme/auth": [
        "packages/auth/src/index.ts",
        "packages/auth/dist/index.d.ts"
      ],
      "@acme/auth/*": [
        "packages/auth/src/*",
        "packages/auth/dist/*"
      ],
      "@acme/platform-core": [
        "packages/platform-core/src/index.ts",
        "packages/platform-core/dist/index.d.ts"
      ],
      "@acme/platform-core/*": [
        "packages/platform-core/src/*",
        "packages/platform-core/dist/*"
      ],
      "@acme/shared-utils": [
        "packages/shared-utils/src/index.ts",
        "packages/shared-utils/dist/index.d.ts"
      ],
      "@acme/shared-utils/*": [
        "packages/shared-utils/src/*",
        "packages/shared-utils/dist/*"
      ],
      "@acme/email": [
        "packages/email/src/index.ts",
        "packages/email/dist/index.d.ts"
      ],
      "@acme/email/*": [
        "packages/email/src/*",
        "packages/email/dist/*"
      ],
      "@acme/email-templates": [
        "packages/email-templates/src/index.ts",
        "packages/email-templates/dist/index.d.ts"
      ],
      "@acme/email-templates/*": [
        "packages/email-templates/src/*",
        "packages/email-templates/dist/*"
