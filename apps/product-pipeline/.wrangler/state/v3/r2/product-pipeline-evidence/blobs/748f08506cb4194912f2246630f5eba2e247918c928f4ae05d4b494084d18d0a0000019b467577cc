e = cardContainer.dataset.slot || ''; const isMshop = slotName === 'mshop-sparkle-snow'; const P = window.P; const viewabilityTracker = {MIN_PERCENT_REQUIRED: 0.5, MIN_TIME_IN_VIEW: 1e3, percentageViewed: 50, startTimeInView: 0, TOP_OBSTRUCTION_HEIGHT: 0, BOTTOM_OBSTRUCTION_HEIGHT: 0, IS_FULLY_OBSTRUCTED: false, /** * Checks if element is visiable on the screen and calls a callback function if it is. * @param {HTMLElement} el - the element to check for visibility. * @param {function} onInView - the callback function */ checkViewability: function(el, onInView) {const box = el.getBoundingClientRect(); const totalArea = box.width * box.height; if (totalArea) {const visiableArea = this.getVisibleAreas(box); const percentVisibleInViewport = visiableArea / totalArea; let percentVisible = percentVisibleInViewport; if (percentVisible >= this.MIN_PERCENT_REQUIRED) {const percentVisibleThroughObstructions = this.getOverlayedObstructions(el); percentVisible = percentVisibleInViewport * percentVisibleThroughObstructions; } this.percentageViewed = percentVisible; if (percentVisible >= this.MIN_PERCENT_REQUIRED) {if (this.startTimeInView === 0) {this.startTimeInView = Date.now(); } onInView(); } else {this.startTimeInView = 0; onInView.cancel(); } } }, calculateVisibleArea: function(topPosition, bottomPosition, leftPosition, rightPosition, effectiveViewportHeight) {const visibleWidth = this.clipToVisibleAreas(leftPosition, rightPosition, window.innerWidth); const visibleHeight = this.clipToVisibleAreas(topPosition, bottomPosition, effectiveViewportHeight); return visibleWidth * visibleHeight; }, /** * Calculates the visiable area of an element * @param {DOMRect Object} b - the bounding box of the element * @returns {number} the visible area of the element */ getVisibleAreas: function(b) {const visibleArea = this.calculateVisibleArea(b.top, b.bottom, b.left, b.right, Math.max(window.innerHeight, 0)); if (visibleArea <= 0 || !P) {return visibleArea; } if (this.IS_FULLY_OBSTRUCTED) {return 0; } try {const effectiveViewportHeight = Math.max( window.innerHeight - this.BOTTOM_OBSTRUCTION_HEIGHT - this.TOP_OBSTRUCTION_HEIGHT, 0 ); return this.calculateVisibleArea( b.top - this.TOP_OBSTRUCTION_HEIGHT, b.bottom - this.TOP_OBSTRUCTION_HEIGHT, b.left, b.right, effectiveViewportHeight ); } catch (error) {return visibleArea; } }, /** * Clips a range to the visible area of the screen. * @param {number} start - the start of the range (i.e, left edge). * @param {number} end - the end of the range (i.e, right edge). * @param {number} limit - the limit of the visible area. * @returns {number} clipped range */ clipToVisibleAreas: function(start, end, limit) {if (start >= 0) {return Math.max(0, Math.min(limit - start, end - start)); } if (end > 0) {return Math.max(0, Math.min(Math.min(end, limit), end - start)); } return 0; }, newObstructionMashApiAvailable: function() {if (!P) {return Promise.resolve(false); } return new Promise((resolve) => {P.when('mash').execute('sb-viewability', (mash) => {if (!mash.appCX?.uiOrchestrator?.getObstructions) {resolve(false); return; } mash.appCX.uiOrchestrator.getObstructions({successCallback: () => {resolve(true); return; }, failCallback: () => {resolve(false); return; }, }); }); }); }, getMashNativeObstruction: function(el, onInView) {return this.shouldGetFullNativeObstruction().then((hasFullNativeObstruction) => {if (hasFullNativeObstruction) {return this.getFullObstructionAndSetUpListeners(el, this.checkViewability.bind(this), onInView); } else if (this.shouldGetTopBottomBarObstruction()) {this.getTopBottomBarHeight(); } return null; }); }, shouldGetFullNativeObstruction: function() {if (!isMshop || "C" !== 'T1') {return Promise.resolve(false); } return this.newObstructionMashApiAvailable().then((isNewObstructionMashApiAvailable) => {return isNewObstructionMashApiAvailable; }); }, shouldGetTopBottomBarObstruction: function() {return isMshop; }, getFullObstructionAndSetUpListeners: function(ele, calculateViewabilityFunc, onInView) {return new Promise((resolve) => {try {P?.when('mash').execute('sb-viewability', (mash) => {mash.appCX?.uiOrchestrator?.getObstructions?.({successCallback: (result) => {this.processObstructionResult(result, ele, onInView); } }); const eventHandler = (mashEvent) => {this.processObstructionResult(mashEvent.detail, ele, onInView, calculateViewabilityFunc); }; if (onInView) {mash.addEventListener('obstructionsChanged', eventHandler); resolve(() => mash.removeEventListener('obstructionsChanged', eventHandler)); } }); } catch (error) {resolve(null); } }); }, getTopBarHeight: function () {return new Promise((resolve) => {if (!P) {resolve(0); return; } try {P.when('MShopChrome').execute('sb-viewability', (ext) => {if (!ext || typeof ext.getChromeInfo !== 'function') {resolve(0); return; } ext.getChromeInfo({successCallback: (result) => {if (result.isTransparentNav) {if ( screen.orientation && screen.orientation.type === 'landscape-primary' && typeof result['landscapeChromeHeight'] === 'number' ) {resolve(result['landscapeChromeHeight']); return; } else if ( screen.orientation && screen.orientation.type === 'portrait-primary' && typeof result['portraitChromeHeight'] === 'number' ) {resolve(result['portraitChromeHeight']); return; } } resolve(0); }, failCallback: () => {resolve(0); }, }); }); } catch (error) {resolve(0); } }) }, getBottomBarHeight: function () {return new Promise((resolve) => {if (!P) {resolve(0); return; } try {P.when('MShopChrome').execute('sb-viewability', (ext) => {if (!ext || typeof ext.getBottomBarsHeight !== 'function') {resolve(0); return; } ext.getBottomBarsHeight({successCallback: (result) => {if (typeof result['visibleHeight'] === 'number') {resolve(result['visibleHeight']); return; } resolve(0); }, failCallback: () => {resolve(0); }, }); }); } catch (error) {resolve(0); } }) }, getTopBottomBarHeight: function() {return Promise.all([this.getTopBarHeight(), this.getBottomBarHeight()]) .then(([topHeight, bottomHeight]) => {this.TOP_OBSTRUCTION_HEIGHT = topHeight; this.BOTTOM_OBSTRUCTION_HEIGHT = bottomHeight; }) }, processObstructionResult: function( obstructionInfo, ele, onInView, calculateViewabilityFunc ) {const { isFullyObstructed, boundingBoxes } = obstructionInfo; this.TOP_OBSTRUCTION_HEIGHT = boundingBoxes?.[0]?.height || 0; this.BOTTOM_OBSTRUCTION_HEIGHT = boundingBoxes?.[1]?.height || 0; this.IS_FULLY_OBSTRUCTED = isFullyObstructed || false; calculateViewabilityFunc?.(ele, onInView); }, getOverlayedObstructions: function(element) {const rect = element.getBoundingClientRect(); const spaceInBetweenCheckRow = "desktop" === 'mobile' ? 20 : 30; const spaceInBetweenCheckCol = "desktop" === 'mobile' ? 30 : 50; let obstructedPoints = 0; const viewportRect = {left: Math.max(0, rect.left), right: Math.min(window.innerWidth, rect.right), top: Math.max(0, rect.top), bottom: Math.min(window.innerHeight, rect.bottom), width: 0, height: 0, }; viewportRect.width = Math.max(0, viewportRect.right - viewportRect.left); viewportRect.height = Math.max(0, viewportRect.bottom - viewportRect.top); const [rows, cols] = [ Math.ceil(viewportRect.height / spaceInBetweenCheckRow), Math.ceil(viewportRect.width / spaceInBetweenCheckCol), ]; if (viewportRect.width === 0 || viewportRect.height === 0) {return 0; } const points = []; for (let i = 0; i < rows; i++) {for (let j = 0; j < cols; j++) {const x = viewportRect.left + (viewportRect.width * j) / cols; const y = viewportRect.top + (viewportRect.height * i) / rows; if ( x >= viewportRect.left && x <= viewportRect.right && y >= viewportRect.top && y <= viewportRect.bottom ) {points.push([x, y]); } } } for (const [x, y] of points) {const elementsAtPoint = document.elementsFromPoint(x, y); for (const el of elementsAtPoint) {if (!(el instanceof HTMLElement)) continue; if (el === element || element.contains(el)) {break; } if (el === document.body || el.tagName === 'HTML') {continue; } const computedStyle = window.getComputedStyle(el); if ( computedStyle.display === 'none' || computedStyle.visibility === 'hidden' || computedStyle.opacity === '0' || computedStyle.pointerEvents === 'none' || parseFloat(computedStyle.opacity) < 0.7 ) {continue; } const backgroundColor = computedStyle.backgroundColor; const isTransparentBackground = backgroundColor === 'transparent' || backgroundColor === 'rgba(0, 0, 0, 0)' || backgroundColor === ''; if (isTransparentBackground && !computedStyle.backgroundImage.includes('url')) {continue; } obstructedPoints++; break; } } if (points.length === 0) {return 0; } return 1 - obstructedPoints / points.length; }, /** * Tracks the viewability of an element and fires a pixel if it is visible for at least MIN_TIME_IN_VIEW milliseconds. * @param {HTMLElement} el - the element to track for viewability */ trackViewability: function(el, pixel) {let self = this; let detachObstructionListener = null; function handleScrollAndClick() {self.checkViewability(el, onInView); } const onInView = this.cancellable(function () {offScroll(); offClick(); if (detachObstructionListener) {detachObstructionListener(); } self.emitViewMetrics(pixel); }, this.MIN_TIME_IN_VIEW); this.getMashNativeObstruction(el, onInView).then((detachFunc) => {detachObstructionListener = detachFunc; }); const offScroll = this.onScroll(handleScrollAndClick, { ms: 25 }); const offClick = this.onClick(handleScrollAndClick, { ms: 25}); handleScrollAndClick(); }, /** * Returns a cancellable function that executes the given function after a specified delay. * @param {function} fn - the function to execute * @param {number} delay - the delay in milliseconds * @returns {function} the cancellable function */ cancellable: function(fn, delay) {let timeoutId; function action() {if(!timeoutId) {timeoutId = window.setTimeout(fn, delay); } }; action.cancel = function() {window.clearTimeout(timeoutId); timeoutId = null; }; return action; }, /** * Higher order function which returns a throttled event handler function for the scroll event * @param {function} fn - the function to be called when the event is triggered * @param {object} options * @returns {function} - throttled event handler function. */ onScroll: function(fn, options) {return this.createThrottledEventHandler('scroll', fn, options); }, onClick: function(fn, options) {return this.createThrottledEventHandler('click', fn, options); }, /** * Returns a throttled event handler function * @param {string} eventName * @param {function} fn - the function to be called when the event is triggered * @param {object} options with delay and element to listen for the event on * @returns {function} - throttled event handler function with specifed event */ createThrottledEventHandler: function(eventName, fn, options) {const ms = options.ms || 25; const el = options.el || window; const throttledFn = this.throttle(fn, ms); el.addEventListener(eventName, throttledFn); return function() {el.removeEventListener(eventName, throttledFn); } }, /** * Returns a throttled version of the function. * @param {function} fn - the function to be throttled * @param {number} delay - the amount of time to delay between function calls * @returns {function} - the throttled function */ throttle: function(fn, delay) {let timeoutId; return function() {const args = arguments; const context = this; if (!timeoutId) {timeoutId = setTimeout(function() {fn.apply(context, args); timeoutId = null; }, delay) } } }, /** * Fires viewable impression and log info in case of error. * @param {string} pixel - the function to be throttled */ emitViewMetrics: function(pixel) {const timeInView = Date.now() - this.startTimeInView; const payload = JSON.stringify({v: {def: 'iab', event: 'VIEWED', p: Math.round(this.percentageViewed * 100), t: parseFloat((timeInView / 1000).toFixed(3)) }, programType: 'SBC', }); const pixelUrlWithPixelType = pixel.substr(-1) === '/' ? pixel + 'v/' : pixel + '/v/'; if (typeof window.fetch === 'function') {fetch(pixelUrlWithPixelType + encodeURI(payload), {method: 'POST', mode: 'no-cors', headers: {'Content-Type': 'application/json', }, body: payload, }) .then((res) => {if (!res.ok && res.status !== 0) {throw Error(res.status + ' ' + res.statusText); } if (window.ue && window.ue.count) {window.ue.count('adsUchihaClientViewPixelCallSuccess', 1); } }) .catch((e) => {if (window.ue && window.ue.count) {window.ue.count('adsUchihaClientViewPixelCallError', 1); } }); } else {/* Handle older browsers such as IE */ var xhr = new XMLHttpRequest(); xhr.open('POST', pixelUrlWithPixelType + encodeURI(payload)); xhr.setRequestHeader('Content-Type', 'application/json'); xhr.onreadystatechange = function() {if (xhr.readyState === 4) {if (xhr.status !== 200) {if (window.ue && window.ue.count) {window.ue.count('adsUchihaClientViewPixelCallError', 1); } } else {if (window.ue && window.ue.count) {window.ue.count('adsUchihaClientViewPixelCallSuccess', 1); } } } }; xhr.onerror = function() {if (window.ue && window.ue.count) {window.ue.count('adsUchihaClientViewPixelCallError', 1); } }; xhr.send(payload); } } }; function onImageLoaded() {if (++numLoaded === numImages) {if (window.ue && window.ue.count) {const time = Date.now() - window.adsUchihaClient["WVi8nX7ZM0gYYfl1kN0BaQ"]; window.ue.count('adsUchihaClientImageViewReady', time); } viewabilityTracker.trackViewability(cardContainer, "//aax-eu-zaz.amazon.fr/x/px/JFlYvJ1-2TNIGGH5dZDdAWkAAAGbRnVmtgoAAAH2AQBvbm9fdHhuX2JpZDIgICBvbm9fdHhuX2ltcDIgICBI5S_p/"); } }; images.forEach(function(image){if (image.complete) {onImageLoaded(); } else {image.addEventListener('load', onImageLoaded, { once: true }); } }) })()</script></div><script>if(window.mix_csa){window.mix_csa('[cel_widget_id="sb-themed-collection-desktop_loom-desktop-top-slot_1"]', '#CardInstance3qC_qvrMVhS9U3hkFqwcFA')('mark', 'be')}</script>
