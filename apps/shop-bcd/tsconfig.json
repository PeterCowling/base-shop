{
  "extends": "@acme/config/tsconfig.app.json",
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": ".",
    "rootDir": "../..",
    "paths": {
      "@/*": [
        "../../packages/ui/src/*",
        "../../packages/i18n/src/*",
        "../../packages/ui/dist/src/*",
        "../../packages/i18n/dist/*",
        "./dist/*"
      ],
      "@ui/*": [
        "../../packages/ui/src/*", "../../packages/ui/dist/src/*"
      ],
      "@ui/src/*": [
        "../../packages/ui/src/*", "../../packages/ui/dist/src/*"
      ],
      "@acme/ui": [
        "../../packages/ui/dist/index",
        "../../packages/ui/dist",
        "../../packages/ui/src/index.ts",
        "../../packages/ui/src"
      ],
      "@acme/ui/*": [
        "../../packages/ui/dist/*", "../../packages/ui/src/*"
      ],
      "@i18n": [
        "../../packages/i18n/src/index.ts", "../../packages/i18n/dist/index"
      ],
      "@i18n/*": [
        "../../packages/i18n/src/*", "../../packages/i18n/dist/*"
      ],
      "@acme/i18n": [
        "../../packages/i18n/src/index.ts", "../../packages/i18n/dist/index"
      ],
      "@acme/i18n/*": [
        "../../packages/i18n/src/*", "../../packages/i18n/dist/*"
      ],
      "@acme/config": [
        "../../packages/config/src/index.ts",
        "../../packages/config/src",
        "../../packages/config/dist/index",
        "../../packages/config/dist"
      ],
      "@acme/config/*": [
        "../../packages/config/src/*", "../../packages/config/dist/*"
      ],
      "@platform-core": [
        "../../packages/platform-core/src/index.ts", "../../packages/platform-core/dist/index"
      ],
      "@platform-core/*": [
        "../../packages/platform-core/src/*", "../../packages/platform-core/dist/*"
      ],
      "@acme/platform-core": [
        "../../packages/platform-core/src/index.ts", "../../packages/platform-core/dist/index"
      ],
      "@acme/platform-core/*": [
        "../../packages/platform-core/src/*", "../../packages/platform-core/dist/*"
      ],
      "@auth": [
        "../../packages/auth/src/index.ts",
        "../../packages/auth/src",
        "../../packages/auth/dist/index",
        "../../packages/auth/dist"
      ],
      "@auth/*": [
        "../../packages/auth/src/*", "../../packages/auth/dist/*"
      ],
      "@date-utils": [
        "../../packages/date-utils/src/index.ts",
        "../../packages/date-utils/src",
        "../../packages/date-utils/dist/index",
        "../../packages/date-utils/dist"
      ],
      "@date-utils/*": [
        "../../packages/date-utils/src/*", "../../packages/date-utils/dist/*"
      ],
      "@acme/date-utils": [
        "../../packages/date-utils/src/index.ts",
        "../../packages/date-utils/src",
        "../../packages/date-utils/dist/index",
        "../../packages/date-utils/dist"
      ],
      "@acme/date-utils/*": [
        "../../packages/date-utils/src/*", "../../packages/date-utils/dist/*"
      ],
      "@acme/email": [
        "../../packages/email/src/index.ts",
        "../../packages/email/src",
        "../../packages/email/dist/index",
        "../../packages/email/dist"
      ],
      "@acme/email/*": [
        "../../packages/email/src/*", "../../packages/email/dist/*"
      ],
      "@acme/lib": [
        "../../packages/lib/src/index.ts",
        "../../packages/lib/src",
        "../../packages/lib/dist/index",
        "../../packages/lib/dist"
      ],
      "@acme/lib/*": [
        "../../packages/lib/src/*", "../../packages/lib/dist/*"
      ],
      "@acme/email-templates": [
        "../../packages/email-templates/src/index.ts",
        "../../packages/email-templates/src",
        "../../packages/email-templates/dist/index",
        "../../packages/email-templates/dist"
      ],
      "@acme/email-templates/*": [
        "../../packages/email-templates/src/*",
        "../../packages/email-templates/dist/*"
      ],
      "@shared-utils": [
        "../../packages/shared-utils/src/index.ts",
        "../../packages/shared-utils/src",
        "../../packages/shared-utils/dist/index",
        "../../packages/shared-utils/dist"
      ],
      "@shared-utils/*": [
        "../../packages/shared-utils/src/*", "../../packages/shared-utils/dist/*"
      ],
      "@acme/shared-utils": [
        "../../packages/shared-utils/src/index.ts",
        "../../packages/shared-utils/src",
        "../../packages/shared-utils/dist/index",
        "../../packages/shared-utils/dist"
      ],
      "@acme/shared-utils/*": [
        "../../packages/shared-utils/src/*", "../../packages/shared-utils/dist/*"
      ],
      "@scripts": [
        "../../scripts/src/index.ts",
        "../../scripts/src",
        "../../scripts/dist/index",
        "../../scripts/dist"
      ],
      "@scripts/*": [
        "../../scripts/src/*", "../../scripts/dist/*"
      ],
      "@acme/types": [
        "../../packages/types/src/index.ts",
        "../../packages/types/src",
        "../../packages/types/dist/index",
        "../../packages/types/dist"
      ],
      "@acme/types/*": [
        "../../packages/types/src/*", "../../packages/types/dist/*"
      ],
      "@acme/sanity": [
        "../../packages/sanity/src/index.ts",
        "../../packages/sanity/src",
        "../../packages/sanity/dist/index",
        "../../packages/sanity/dist"
      ],
      "@acme/sanity/*": [
        "../../packages/sanity/src/*", "../../packages/sanity/dist/*"
      ],
      "@acme/stripe": [
        "../../packages/stripe/src/index.ts",
        "../../packages/stripe/src",
        "../../packages/stripe/dist/index",
        "../../packages/stripe/dist"
      ],
      "@acme/stripe/*": [
        "../../packages/stripe/src/*", "../../packages/stripe/dist/*"
      ],
      "@acme/zod-utils": [
        "../../packages/zod-utils/src/index.ts",
        "../../packages/zod-utils/src",
        "../../packages/zod-utils/dist/index",
        "../../packages/zod-utils/dist"
      ],
      "@acme/zod-utils/*": [
        "../../packages/zod-utils/src/*", "../../packages/zod-utils/dist/*"
      ]
    },
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "resolveJsonModule": true,
    "strictNullChecks": true
  },
  "include": [
    "src/**/*",
    "src/app/.well-known/**/*",
    ".next/types/**/*.ts",
    "shop.json",
    "next-env.d.ts",
    "src/**/*.json",
    "routes/**/*",
    "middleware.ts"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "../../apps-script",
    ".next",
    "../../packages/**/__tests__/**",
    "../../test",
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/*.spec.*",
    "**/*.stories.*"
  ],
  "references": [
    {
      "path": "../../packages/ui"
    },
    {
      "path": "../../packages/i18n"
    },
    {
      "path": "../../packages/platform-core"
    },
    {
      "path": "../../packages/auth"
    },
    {
      "path": "../../packages/date-utils"
    },
    {
      "path": "../../packages/shared-utils"
    },
    {
      "path": "../../packages/types"
    },
    {
      "path": "../../packages/zod-utils"
    },
    {
      "path": "../../scripts"
    },
    {
      "path": "../../packages/config"
    },
    {
      "path": "../../packages/sanity"
    },
    {
      "path": "../../packages/email"
    },
    {
      "path": "../../packages/stripe"
    },
    {
      "path": "../../packages/lib"
    },
    {
      "path": "../../packages/email-templates"
    }
  ]
}
