{
  "name": "@apps/brikette",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--require ./scripts/ssr-polyfills.cjs\" next dev -p 3012",
    "prebuild": "pnpm --filter @acme/telemetry clean && pnpm --filter @acme/telemetry build",
    "build": "cross-env NODE_OPTIONS=\"--require ./scripts/ssr-polyfills.cjs\" next build",
    "start": "cross-env NODE_OPTIONS=\"--require ./scripts/ssr-polyfills.cjs\" next start -p 3012",
    "lint": "echo 'Lint temporarily disabled - see eslint.config.mjs ignores'",
    "pretypecheck": "pnpm --filter @acme/telemetry build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "pnpm exec jest --ci --runInBand --detectOpenHandles --passWithNoTests",
    "generate:url-fixtures": "cd /tmp && npx tsx $OLDPWD/scripts/generate-url-fixtures.ts"
  },
  "dependencies": {
    "@acme/design-system": "workspace:*",
    "@acme/next-config": "workspace:*",
    "@acme/telemetry": "workspace:*",
    "@acme/ui": "workspace:*",
    "@headlessui/react": "^2.2.3",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "i18next": "^24.2.3",
    "i18next-resources-to-backend": "^1.2.1",
    "lucide-react": "^0.511.0",
    "next": "^15.3.8",
    "react": "19.2.1",
    "react-datepicker": "^8.3.0",
    "react-dom": "19.2.1",
    "react-i18next": "^15.5.3",
    "swiper": "^11.2.6",
    "web-vitals": "^5.0.2",
    "zod": "^3.25.73"
  }
}
