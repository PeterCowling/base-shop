{
  "name": "@acme/lib",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest --ci --runInBand --detectOpenHandles --config ../../jest.config.cjs"
  },
  "devDependencies": {
    "next": "^15.3.4"
  },
  "dependencies": {
    "@acme/config": "workspace:^",
    "@acme/zod-utils": "workspace:^",
    "@acme/platform-core": "workspace:^"
  }
}
