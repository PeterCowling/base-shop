{
  "name": "@acme/config",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./env": {
      "types": "./dist/env/index.d.ts",
      "import": "./dist/env/index.js",
      "default": "./dist/env/index.js"
    },
    "./env/core": {
      "types": "./dist/env/core.d.ts",
      "import": "./dist/env/core.js",
      "default": "./dist/env/core.js"
    },
    "./env/auth": {
      "types": "./dist/env/auth.d.ts",
      "import": "./dist/env/auth.js",
      "default": "./dist/env/auth.js"
    },
    "./env/cms": {
      "types": "./dist/env/cms.d.ts",
      "import": "./dist/env/cms.js",
      "default": "./dist/env/cms.js"
    },
    "./env/email": {
      "types": "./dist/env/email.d.ts",
      "import": "./dist/env/email.js",
      "default": "./dist/env/email.js"
    },
    "./env/payments": {
      "types": "./dist/env/payments.d.ts",
      "import": "./dist/env/payments.js",
      "default": "./dist/env/payments.js"
    },
    "./env/shipping": {
      "types": "./dist/env/shipping.d.ts",
      "import": "./dist/env/shipping.js",
      "default": "./dist/env/shipping.js"
    },
    "./env-schema": {
      "types": "./dist/env-schema.d.ts",
      "import": "./dist/env-schema.js",
      "default": "./dist/env-schema.js"
    },
    "./jest.preset.cjs": "./jest.preset.cjs",
    "./tsconfig.app.json": "./tsconfig.app.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist tsconfig.tsbuildinfo && pnpm --filter @acme/zod-utils run build && pnpm run build:stubs",
    "prepare": "pnpm run build:stubs",
    "build:stubs": "node ./scripts/generate-env-stubs.mjs",
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "clean": "rimraf dist *.tsbuildinfo",
    "test": "jest packages/config/__tests__ packages/config/src/env/__tests__ --passWithNoTests --runInBand --config jest.config.cjs",
    "fix:env": "node ./scripts/fix-config-env.mjs",
    "verify:env": "node ./scripts/verify-config-env.mjs",
    "lint": "eslint ."
  },
  "dependencies": {
    "@acme/zod-utils": "workspace:^"
  }
}
