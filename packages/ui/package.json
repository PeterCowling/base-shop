{
  "name": "@acme/ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components/cms/blocks/CollectionSection.server": {
      "types": "./dist/components/cms/blocks/CollectionSection.server.d.ts",
      "import": "./dist/components/cms/blocks/CollectionSection.server.js"
    },
    "./components/cms/blocks/CollectionSection.client": {
      "types": "./dist/components/cms/blocks/CollectionSection.client.d.ts",
      "import": "./dist/components/cms/blocks/CollectionSection.client.js"
    },
    "./components/cms/blocks/HeaderSection": {
      "types": "./dist/components/cms/blocks/HeaderSection.d.ts",
      "import": "./dist/components/cms/blocks/HeaderSection.js"
    },
    "./components/cms/blocks/FooterSection": {
      "types": "./dist/components/cms/blocks/FooterSection.d.ts",
      "import": "./dist/components/cms/blocks/FooterSection.js"
    },
    "./components/cms/blocks/CurrencySelector": {
      "types": "./dist/components/cms/blocks/CurrencySelector.d.ts",
      "import": "./dist/components/cms/blocks/CurrencySelector.js"
    },
    "./components/cms/blocks/RentalAvailabilitySection": {
      "types": "./dist/components/cms/blocks/RentalAvailabilitySection.d.ts",
      "import": "./dist/components/cms/blocks/RentalAvailabilitySection.js"
    },
    "./components/cms/blocks/RentalTermsSection": {
      "types": "./dist/components/cms/blocks/RentalTermsSection.d.ts",
      "import": "./dist/components/cms/blocks/RentalTermsSection.js"
    },
    "./components/cms/blocks/StructuredDataSection": {
      "types": "./dist/components/cms/blocks/StructuredDataSection.d.ts",
      "import": "./dist/components/cms/blocks/StructuredDataSection.js"
    },
    "./components/cms/blocks/ConsentSection": {
      "types": "./dist/components/cms/blocks/ConsentSection.d.ts",
      "import": "./dist/components/cms/blocks/ConsentSection.js"
    },
    "./components/cms/blocks/AnalyticsPixelsSection": {
      "types": "./dist/components/cms/blocks/AnalyticsPixelsSection.d.ts",
      "import": "./dist/components/cms/blocks/AnalyticsPixelsSection.js"
    },
    "./components/cms/blocks/RentalDemoProvider.client": {
      "types": "./dist/components/cms/blocks/RentalDemoProvider.client.d.ts",
      "import": "./dist/components/cms/blocks/RentalDemoProvider.client.js"
    },
    "./components/platform/*": {
      "types": "./dist/components/platform/*.d.ts",
      "import": "./dist/components/platform/*.js"
    },
    "./account": {
      "types": "./dist/account.d.ts",
      "import": "./dist/account.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf dist *.tsbuildinfo",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "pnpm exec jest --ci --runInBand --detectOpenHandles --config ./jest.config.cjs --coverage",
    "test:pkg": "pnpm exec jest --ci --runInBand --detectOpenHandles --config ./jest.config.cjs --coverage",
    "test:quick": "JEST_DISABLE_COVERAGE_THRESHOLD=1 pnpm exec jest --runInBand --detectOpenHandles --config ./jest.config.cjs",
    "lint": "eslint ."
  },
  "dependencies": {
    "@acme/page-builder-core": "workspace:*",
    "@acme/config": "workspace:*",
    "@acme/date-utils": "workspace:*",
    "@acme/i18n": "workspace:*",
    "@acme/platform-core": "workspace:*",
