{
  "name": "@acme/types",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "zod": "^3.25.73"
  },
  "devDependencies": {
    "next": "^15.3.9"
  },
  "type": "module",
  "files": [
    "dist",
    "src/global.d.ts"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "clean": "rimraf dist *.tsbuildinfo",
    "lint": "eslint .",
    "test": "pnpm exec jest --ci --runInBand --detectOpenHandles --config ./jest.config.cjs"
  },
  "exports": {
    "./package.json": "./package.json",
    "./page": {
      "types": "./dist/page/index.d.ts",
      "import": "./dist/page/index.js",
      "default": "./dist/page/index.js"
    },
    "./page/*": {
      "types": "./dist/page/*.d.ts",
      "import": "./dist/page/*.js",
      "default": "./dist/page/*.js"
    },
    "./settings": {
      "types": "./dist/settings/index.d.ts",
      "import": "./dist/settings/index.js",
      "default": "./dist/settings/index.js"
    },
    "./settings/*": {
      "types": "./dist/settings/*.d.ts",
      "import": "./dist/settings/*.js",
      "default": "./dist/settings/*.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme/index.js",
      "default": "./dist/theme/index.js"
    },
    "./theme/*": {
      "types": "./dist/theme/*.d.ts",
      "import": "./dist/theme/*.js",
      "default": "./dist/theme/*.js"
    },
    "./ds": {
      "types": "./dist/ds/index.d.ts",
      "import": "./dist/ds/index.js",
      "default": "./dist/ds/index.js"
    },
    "./ds/*": {
      "types": "./dist/ds/*.d.ts",
      "import": "./dist/ds/*.js",
      "default": "./dist/ds/*.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  }
}
