{
  "name": "cms-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.12.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "start": "turbo run start --parallel",
    "lint": "turbo run lint",
    "lint:all": "eslint \"{apps,packages,src}/**/*.{ts,tsx,js,jsx}\"",
    "typecheck": "tsc -b --pretty",
    "test": "turbo run test",
    "e2e": "playwright test",
    "i18n:extract": "next-intl extract",
    "i18n:check": "ts-node scripts/check-i18n-schemas.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "preview": "wrangler pages dev",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.1",
    "next": "15.3.4",
    "next-auth": "4.24.11",
    "next-intl": "^3.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "stripe": "^18.2.1",
    "ulid": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.1",
    "@storybook/addon-a11y": "^9.0.13",
    "@storybook/addon-themes": "^9.0.13",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/jest": "^0.2.3",
    "@storybook/nextjs-vite": "^9.0.13",
    "@storybook/testing-react": "^2.0.1",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-fetch": "^4.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-testing-library": "^7.5.3",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "nock": "^13.4.0",
    "playwright": "^1.53.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "turbo": "^1.13.0",
    "typescript": "^5.1.0",
    "vite": "^7.0.0",
    "wrangler": "^4.20.4"
  },
  "pnpm": {
    "overrides": {
      "@types/google-apps-script@*": "npm:@esbuild-plugins/empty@1.0.0"
    }
  }
}
