{
  "name": "@acme/types",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@acme/i18n": "workspace:*"
  },
  "devDependencies": {
    "next": "^15.3.5"
  },
  "type": "module",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf dist *.tsbuildinfo",
    "lint": "eslint .",
    "test": "pnpm exec jest --ci --runInBand --detectOpenHandles --config ./jest.config.cjs"
  },
  "exports": {
    "./settings": {
      "types": "./src/settings/index.ts",
      "import": "./src/settings/index.ts",
      "default": "./src/settings/index.ts"
    },
    "./settings/*": {
      "types": "./src/settings/*.ts",
      "import": "./src/settings/*.ts",
      "default": "./src/settings/*.ts"
    },
    "./theme": {
      "types": "./src/theme/index.ts",
      "import": "./src/theme/index.ts",
      "default": "./src/theme/index.ts"
    },
    "./theme/*": {
      "types": "./src/theme/*.ts",
      "import": "./src/theme/*.ts",
      "default": "./src/theme/*.ts"
    },
    "./ds": {
      "types": "./src/ds/index.ts",
      "import": "./src/ds/index.ts",
      "default": "./src/ds/index.ts"
    },
    "./ds/*": {
      "types": "./src/ds/*.ts",
      "import": "./src/ds/*.ts",
      "default": "./src/ds/*.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts",
      "default": "./src/*.ts"
    },
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  }
}
