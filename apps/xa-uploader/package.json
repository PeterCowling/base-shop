{
  "name": "@apps/xa-uploader",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3020",
    "build": "pnpm exec next build",
    "start": "next start -p 3020",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "bash ../../scripts/tests/run-governed-test.sh -- jest -- --config ./jest.config.cjs",
    "test:api": "bash ../../scripts/tests/run-governed-test.sh -- jest -- --config ./jest.config.cjs --runInBand --testPathPattern=\"src/app/api/.*/__tests__/route.test.ts\"",
    "test:local": "bash ../../scripts/tests/run-governed-test.sh -- jest -- --config ./jest.config.cjs --runInBand --roots ./src --testPathPattern=\"src/(app/api|components/catalog)/\"",
    "test:e2e": "pnpm exec playwright test e2e/catalog-console.spec.ts --reporter=list"
  },
  "dependencies": {
    "@acme/lib": "workspace:*",
    "@acme/next-config": "workspace:*",
    "@acme/tailwind-config": "workspace:*",
    "@acme/template-app": "workspace:*",
    "@themes/base": "workspace:*",
    "fast-csv": "^5.0.5",
    "minimatch": "^9.0.5",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "yazl": "^3.3.1",
    "zod": "^3.25.73"
  },
  "devDependencies": {
    "@types/yauzl": "^2.10.3",
    "@types/yazl": "^3.3.0",
    "next": "^16.1.6",
    "typescript": "5.8.3",
    "yauzl": "^2.10.0"
  }
}
