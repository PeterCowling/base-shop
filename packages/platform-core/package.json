{
  "name": "@platform-core",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./createShop": {
      "types": "./dist/createShop/index.d.ts",
      "import": "./dist/createShop/index.js",
      "default": "./dist/createShop/index.js"
    },
    "./themeTokens": {
      "types": "./dist/themeTokens/index.d.ts",
      "import": "./dist/themeTokens/index.js",
      "default": "./dist/themeTokens/index.js"
    },
    "./products": {
      "types": "./dist/products/index.d.ts",
      "import": "./dist/products/index.js",
      "default": "./dist/products/index.js"
    },
    "./analytics": {
      "types": "./dist/analytics/index.d.ts",
      "import": "./dist/analytics/index.js",
      "default": "./dist/analytics/index.js"
    },
    "./dataRoot": {
      "types": "./src/dataRoot.d.ts",
      "import": "./src/dataRoot.js",
      "default": "./src/dataRoot.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    },
    "./shops": {
      "types": "./dist/shops/index.d.ts",
      "import": "./dist/shops/index.js",
      "default": "./dist/shops/index.js"
    },
    "./repositories/*": {
      "types": "./dist/repositories/*.d.ts",
      "import": "./dist/repositories/*.js",
      "default": "./dist/repositories/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf dist *.tsbuildinfo"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@acme/config": "workspace:^"
  }
}
