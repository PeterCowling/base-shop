{
  "name": "@acme/config",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./env": {
      "types": "./dist/env/index.d.ts",
      "import": "./dist/env/index.js",
      "default": "./dist/env/index.js"
    },
    "./env/*": {
      "types": "./dist/env/*.d.ts",
      "import": "./dist/env/*.js",
      "default": "./dist/env/*.js"
    },
    "./jest.preset.cjs": "./jest.preset.cjs",
    "./tsconfig.app.json": "./tsconfig.app.json"
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "prebuild": "pnpm run build:stubs && pnpm --filter @acme/zod-utils run build",
    "build:stubs": "node ./scripts/generate-env-stubs.mjs",
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json -w",
    "clean": "rimraf dist *.tsbuildinfo",
    "test": "jest packages/config/__tests__ --passWithNoTests --runInBand --config jest.preset.cjs"
  },
  "dependencies": {
    "@acme/zod-utils": "workspace:^"
  }
}
