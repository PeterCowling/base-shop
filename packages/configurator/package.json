{
  "name": "@acme/configurator",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "configurator": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./providers": {
      "types": "./src/providers.ts",
      "import": "./src/providers.ts",
      "default": "./src/providers.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf dist *.tsbuildinfo",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250702.0",
    "@types/node": "^24.0.10",
    "vite": "^7.0.0",
    "wrangler": "^4.20.4"
  }
}
