{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "types": ["@testing-library/jest-dom", "jest", "node"],
    "noEmit": false,
    "noEmitOnError": false,
    "rootDir": ".",
    "outDir": ".ts-jest",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "esModuleInterop": true
  },
  "include": ["src/**/*", "__tests__/**/*"],
  "exclude": []
}
