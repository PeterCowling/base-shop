  "scripts": {
    "dev": "next dev -p 3020",
    "build": "next build",
    "start": "next start -p 3020",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "guard:no-external-reservation-grid": "! rg -n \"from ['\\\\\\\"]@daminort/reservation-grid['\\\\\\\"]|require\\\\(['\\\\\\\"]@daminort/reservation-grid['\\\\\\\"]\\\\)|import\\\\(['\\\\\\\"]@daminort/reservation-grid['\\\\\\\"]\\\\)|declare module \\\"@daminort/reservation-grid\\\"\" src -S",
    "test": "pnpm exec jest --ci --runInBand --detectOpenHandles --passWithNoTests --config ./jest.config.cjs",
    "test:rules": "pnpm dlx firebase-tools@13.35.1 emulators:exec --only database --project reception-rules-test --config ../../firebase.json \"MSW_DISABLE=1 pnpm exec jest --ci --runInBand --detectOpenHandles --passWithNoTests -- src/rules/__tests__/databaseRules.test.ts\""
  },
  "dependencies": {
    "@acme/design-system": "workspace:*",
    "@acme/mcp-server": "workspace:*",
    "@acme/ui": "workspace:*",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "firebase": "^11.8.0",
    "framer-motion": "^12.0.0",
    "lucide-react": "^0.540.0",
    "next": "^16.1.6",
    "react": "19.2.1",
    "react-day-picker": "^9.10.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "19.2.1",
    "react-transition-group": "^4.4.5",
    "sweetalert2": "^11.22.2"
