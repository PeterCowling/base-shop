{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "dependsOn": ["^dev"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".vercel/**", "dist/**", ".next/**"]
    },
    "lint": {},
    "test": {
      "dependsOn": [],
      "cache": false,
      "outputs": ["coverage/**"]
    }
  }
}
